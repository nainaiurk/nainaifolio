{
  "version": 2,
  "routes": [
    { "handle": "filesystem" },

    { "src": "^/$", "headers": { "Cache-Control": "no-cache, must-revalidate, max-age=0" }, "dest": "/index.html" },
    { "src": "^/index.html$", "headers": { "Cache-Control": "no-cache, must-revalidate, max-age=0" }, "dest": "/index.html" },
    { "src": "^/main.dart.js$", "headers": { "Cache-Control": "public, max-age=31536000, immutable" }, "dest": "/main.dart.js" },
    { "src": "^/flutter_service_worker.js$", "headers": { "Cache-Control": "no-cache, max-age=0, must-revalidate" }, "dest": "/flutter_service_worker.js" },
    { "src": "^/assets/(.*)$", "headers": { "Cache-Control": "public, max-age=31536000, immutable" }, "dest": "/assets/$1" },

    { "src": "/.*", "dest": "/index.html" }
  ]
}
